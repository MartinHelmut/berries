{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.5.4",
  "pnpmVersion": "2.25.6",
  "pnpmOptions": {
    "strictPeerDependencies": false
  },
  "nodeSupportedVersionRange": ">=8.9.4 <11.0.0",
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,
  "projects": [
    {
      "packageName": "@berries/acai-cli",
      "projectFolder": "apps/acai-cli",
      "shouldPublish": true
    },
    {
      "packageName": "@berries/acai",
      "projectFolder": "libraries/acai",
      "shouldPublish": true
    },
    {
      "packageName": "@berries/website",
      "projectFolder": "apps/website"
    }
  ]
}
