{
  "name": "@berries/acai",
  "version": "1.0.0",
  "description": "Predict bugs in git repositories based on commit messages.",
  "main": "index.js",
  "scripts": {
    "docs": "jsdoc -c .jsdoc.json --verbose && serve docs",
    "lint": "npx eslint '**/*.js'",
    "test": "jest --env=node",
    "test:coverage": "jest --env=node --coverage src && cat ./coverage/lcov.info | coveralls",
    "fmt:check": "npx prettier --tab-width 4 --single-quote --list-different 'src/**/*.js'"
  },
  "keywords": [
    "predict",
    "bugs",
    "git",
    "commit",
    "acai",
    "berries"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de> (http://martin-fieber.de)",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "index.js",
    "src/",
    "__mocks__"
  ],
  "dependencies": {
    "minimatch": "3.0.4",
    "nodegit": "0.20.3"
  },
  "devDependencies": {
    "coveralls": "3.0.0",
    "eslint": "4.13.1",
    "eslint-config-prettier": "2.9.0",
    "jest": "22.0.0",
    "jsdoc": "3.5.5",
    "serve": "6.4.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
