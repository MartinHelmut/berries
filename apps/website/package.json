{
  "name": "@berries/website",
  "version": "1.1.6",
  "private": true,
  "description": "The awesome berries documentation website.",
  "scripts": {
    "dev": "next",
    "build": "next build && next export -s",
    "start": "next start",
    "docs": "yarn start",
    "lint": "eslint '**/*.js' && stylelint 'components/*.jsx'",
    "test": "jest --passWithNoTests",
    "format": "prettier --write \"**/*.{js,css,md,json}\"",
    "format-check": "prettier --list-different \"**/*.{js,css,md,json}\""
  },
  "keywords": [
    "website",
    "berries",
    "documentation"
  ],
  "author": "Martin Helmut Fieber <info@martin-fieber.de> (https://martin-fieber.de)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MartinHelmut/berries.git"
  },
  "bugs": {
    "url": "https://github.com/MartinHelmut/berries/issues"
  },
  "homepage": "https://pick-your-berries.net",
  "dependencies": {
    "next": "9.0.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime-corejs2": "7.5.5",
    "babel-jest": "24.8.0",
    "babel-plugin-styled-components": "1.10.6",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "regenerator-runtime": "0.13.3",
    "stylelint": "10.1.0",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0"
  }
}
